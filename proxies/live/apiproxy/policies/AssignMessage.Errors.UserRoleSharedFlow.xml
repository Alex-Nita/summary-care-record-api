<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.Errors.UserRoleSharedFlow">
    <AssignVariable>
        <Name>scr.error.code</Name>
        <Template>{validation.statusCode}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>scr.error.coding.code</Name>
        <Value>INVALID_VALUE</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>scr.error.coding.display</Name>
        <Value>Provided value is invalid</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>scr.error.diagnostics</Name>
        <Template>{validation.errorDescription}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>