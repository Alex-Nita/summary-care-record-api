<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.PostMethodNotAllowAppRestricted">
    <DisplayName>RaiseFault.PostMethodNotAllowAppRestricted</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="text/plain"/>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Application-restricted access is not available on this POST request</ReasonPhrase>
        </Set>
    <AssignVariable>
      <Name>scr.error.diagnostics</Name>
      <Template>Your app has insufficient permissions to use this method. Please contact support.</Template>
    </AssignVariable>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>