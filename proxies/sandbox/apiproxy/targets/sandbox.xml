<TargetEndpoint name="apim">
  <FaultRules/>
  <PreFlow>
    <Request>
      <Step>
        <Name>AssignMessage.AddAsidHeader</Name>
      </Step>
      <Step>
        <Name>AssignMessage.AddOriginalRequestIp</Name>
      </Step>
